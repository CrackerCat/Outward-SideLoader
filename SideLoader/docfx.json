{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SideLoader/**.csproj"
          ]
        }
      ],
      "dest": "_docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "filter": "_docs/filterconfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "_docs/api/**.yml",
          "_docs/api/index.md"
        ]
      },
      {
        "files": [
          "_docs/articles/**.md",
          "_docs/articles/**/toc.yml",
          "_docs/toc.yml",
          "_docs/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "_docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "_docs/apidoc/**.md"
        ],
        "exclude": [
          "_docs/obj/**",
          "_docs/_site/**"
        ]
      }
    ],
    "dest": "_docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
        "default",
        "_docs/templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}